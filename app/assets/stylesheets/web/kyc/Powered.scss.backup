.container-powered{
  padding-bottom: 50px;
  .powered-desc {
    background-color: #a4dee0;
    padding-top: 35px;
    h2{
      font-size: 44px;
      font-weight: 300;
      color: #fff;
      text-align: center;
      margin: 0;
      letter-spacing: 2px;
    }
    p{
      color: #597a84;
      font-weight: 300;
      margin: 20px 0 30px;
      letter-spacing: 0.4px;
      text-align: center;
      font-size: 16px;
    }

  }
  .read-more{
    letter-spacing: 1px;
    line-height: 30px;
    color: #c0993a;
    border-bottom: 1px solid #c0993a;
    &:hover, &:focus{
      outline: none;
      text-decoration: none;
    }
  }
  .square-btn{
    margin: 15px auto 0;
    line-height: 40px;
    padding: 0 20px;
  }
