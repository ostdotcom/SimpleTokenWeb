.table-head{
  height: 61.5px;

}
.table-head-presale{
  height: 61.5px;
  background-color: #ffffff;
  opacity: 0.6;
  width: 160px;
  .title{
    color: #234359;
    font-weight: bold;
    font-size: 18px;
  }

}
.table-head-public{
  width: 160px;
  height: 61.5px;
  object-fit: contain;
  background-color: #ffffff;
  .title{
    color: #234359;
    font-weight: bold;
    font-size: 18px;
  }





}

.active{
  div{
    background-color: #f3fbfd;
    min-height: 60px;
    @media (min-width: 667px) {

    }
  }
}
.transparent{
  div{
    background-color: #ffffff;
    min-height: 60px;
  }
}
