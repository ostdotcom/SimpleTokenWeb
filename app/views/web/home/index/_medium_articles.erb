<div class="container-press paddingTop57" id="latestupdates">

  <div class="container">

    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
        <div class="latest-updates-carousel with-placeholder">
          <% for i in 1..3 do %>
            <div class="carousel-item">
              <div class="thumbnail">
                <div class="video-container">
                  <img class="thumb-img" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAJCAYAAAFMLZykAAAABGdBTUEAALGPC/xhBQAAABFJREFUKBVjYBgFoyFA1RAAAAJTAAEABdnJAAAAAElFTkSuQmCC" />
                </div>
                <div class="caption">
                  <h4></h4>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>

  </div>

</div>


<script type="custom" id="latest-update-carousel-item">
    <div class="carousel-item">
      <div class="thumbnail">
        <a href="__link__" target="_blank">
          <img src="__image__" alt="__title__" style="width: 100%;">
          <div class="caption">
            <h4>__title__</h4>
          </div>
        </a>
      </div>
    </div>
</script>