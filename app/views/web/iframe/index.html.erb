<button class="btn btn-primary test-post">Test post request</button>
<button class="btn btn-primary test-get">Test get request</button>
<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>


<script type="text/javascript">

  window.addEventListener("message", receiveMessage, false);
  function receiveMessage(event)
  {
      if (event.origin !== "https://iframekyc.stagingost.com")
          return;
      alert("data received successfully. Data: " + event.data.message);
  }

</script>


<div>
  <% if Rails.env.staging?
     src_1 = "https://iframekyc.stagingost.com/kyc"
    else
     src_1 = "http://iframekyc.developmentost.com:8080/kyc"
  %>

  <iframe width="100%" height="700px"   src="<%= src_1 %>"  ></iframe>

<!--  frameBorder="0"-->
</div>