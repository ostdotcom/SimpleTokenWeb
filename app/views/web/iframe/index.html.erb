<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>


<script type="text/javascript">

    window.addEventListener("message", receiveMessage, false);

    function receiveMessage(event) {
        if (!["http://iframekyc.developmentost.com:8080", "https://iframekyc.stagingost.com"].includes(event.origin))
         return;
        alert("data received successfully. Data: " + event.data.message.a + " ===" + event.origin);
    }

</script>
<% if Rails.env.staging?
     src = "https://iframekyc.stagingost.com/kyc"
   else
     src = "http://iframekyc.developmentost.com:8080/kyc"
   end
%>

  <div>
    <iframe width="100%" height="700px" src="<%= src %>"></iframe>

    <!--  frameBorder="0"-->
  </div>