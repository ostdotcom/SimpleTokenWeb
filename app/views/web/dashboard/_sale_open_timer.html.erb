<div class="container-fluid dashboard-page margin35auto">
  <div class="row">
    <div class="col-xs-12">
      <!--<div class="text-center">-->

        <div class="registration-timer">
          <h2><%= @presenter_obj.is_early_access_mode_on_for_user? ? 'early access sale' : 'public sale' %> <%= @presenter_obj.is_sale_live_for_user? ? 'is live' : 'starts in' %></h2>
          <% if @presenter_obj.is_sale_live_for_user? %>
              <div class="time-remain text-center" style="margin-bottom: 20px">&mdash;&nbsp;Time Remaining&nbsp;&mdash;</div>
          <% end %>

          <%=
              render partial: '/shared/web/common/marketing_countdown_timer',
                     locals: {
                             timestamp: (@presenter_obj.countdown_timestamp - Time.now).to_i * 1000
                     }
          %>
          <%# if @presenter_obj.is_sale_live_for_user? && !@presenter_obj.is_early_access_mode_on_for_user? %>
              <div class="time-remain text-center" style="margin-top: 25px">sale open till tokens last</div>
          <%# end %>

        </div>
      <!--</div>-->
    </div>
  </div>
</div>