<div id="banner-content"></div>
<%= render :partial => "/shared/web/common/loggedin_header", locals: {user: @presenter_obj.user} %>
<div class="text-center">
  <img src="https://d36lai1ppm9dxu.cloudfront.net/assets/logo/simple-token-dashboard-logo-1x.png" width="202"/>
</div>

<% if @presenter_obj.has_sale_ended? %>
    <%= render :partial => "/web/dashboard/token_sale_ends" %>
    <%= render :partial => "/web/dashboard/progress_bar_with_sale_stats" %>
    <% if !@presenter_obj.is_kyc_denied? %>
        <img src="https://d36lai1ppm9dxu.cloudfront.net/assets/image/join-team-divider-3x.png" width="124" style="margin: 35px auto 30px;display: block;">
    <% end %>
    <%= render :partial => "/web/dashboard/forgot_eth_address" %>
<% elsif @presenter_obj.has_sale_paused? %>
    <%= render :partial => "/web/dashboard/token_sale_pause" %>
    <%= render :partial => "/web/dashboard/progress_bar_with_sale_stats" %>
    <% if !@presenter_obj.is_kyc_denied? %>
        <img src="https://d36lai1ppm9dxu.cloudfront.net/assets/image/join-team-divider-3x.png" width="124" style="margin: 35px auto 30px;display: block;">
    <% end %>
    <%= render :partial => "/web/dashboard/forgot_eth_address" %>
<% elsif @presenter_obj.is_sale_ongoing_for_user? %>

    <% if @presenter_obj.can_purchase? %>
        <%= render :partial => "/web/dashboard/sale_open_timer" %>
        <%= render :partial => "/web/dashboard/ethereum_deposit_address" %>
        <%= render :partial => "/web/dashboard/progress_bar_with_sale_stats" %>
        <%= render :partial => "/web/dashboard/kyc_status" %>
        <img src="https://d36lai1ppm9dxu.cloudfront.net/assets/image/join-team-divider-3x.png" width="124" style="margin: 35px auto 30px;display: block;">
        <%= render :partial => "/web/dashboard/forgot_eth_address" %>
    <% else %>
        <%= render :partial => "/web/dashboard/sale_open_timer" %>
        <%= render :partial => "/web/dashboard/kyc_status" %>
        <%= render :partial => "/web/dashboard/progress_bar_with_sale_stats" %>
        <img src="https://d36lai1ppm9dxu.cloudfront.net/assets/image/join-team-divider-3x.png" width="124" style="margin: 35px auto 30px;display: block;">
        <%= render :partial => "/web/dashboard/forgot_eth_address" %>
        <%= render :partial => "/web/dashboard/question_on_sale" %>
    <% end %>

<% else %>
    <%= render :partial => "/web/dashboard/sale_open_timer" %>
    <%= render :partial => "/web/dashboard/kyc_status" %>
    <img src="https://d36lai1ppm9dxu.cloudfront.net/assets/image/join-team-divider-3x.png" width="124" style="margin: 35px auto 30px;display: block;">
    <%= render :partial => "/web/dashboard/forgot_eth_address" %>
    <%= render :partial => "/web/dashboard/question_on_sale" %>
<% end %>

<%= render :partial => "/web/dashboard/telegram" %>


<%
   if @presenter_obj.is_double_optin_token_present?
     content_for :end_js do
%>
        <script type="text/javascript">
            $(document).ready(function () {
                simpletoken.utils.errorHandling.showSuccessAlert("Your Account is Activated");
            });
        </script>

    <%
       end
       end
    %>
