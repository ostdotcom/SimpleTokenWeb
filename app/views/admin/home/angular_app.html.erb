<%
    initialConfig = {
        client_setup: @resp_data['client_setup'],
        admin: @resp_data['admin']
    }.to_json

%>

<app-root id="main_app_root"></app-root>
<script text="text/javascript">
  (function () {

    var initData = <%= raw initialConfig %> ;
    console.log("initData ====" , initData);
    var main_app_root = document.getElementById("main_app_root");
    main_app_root.dataset.initDataJson = JSON.stringify( initData );
  })();
</script>

