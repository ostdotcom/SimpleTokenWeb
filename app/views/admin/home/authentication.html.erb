<div class="container sign-up-form">
  <div class="row">
    <div class="col-xs-12">
      <div class="text-center">
        <a href="/" title="Simple Token">
          <img src="//d3attjoi5jlede.cloudfront.net/simpletoken/logo/logo-with-tagline-mobile.png" width="211"/>
        </a>
      </div>
      <div class="simple-token-form">
        <h1 class="text-center form-header">Multi Factor Authentication</h1>
        <div class="form-sub-header">Please enter an MFA code to complete sign-in.</div>
        <form class="user-form" method="" action="" id="userSignupForm">
          <div class="form-group">
            <label for="email">mfa code:</label>
            <input type="text" name="email" id="email" value="" class="form-control" required="" autofocus tabindex="1">
            <span id="err_email" class="error"></span>
          </div>
          <div class="form-group user-form-footer">
            <button type="submit" class="btn st-btn" id="" tabindex="2" title="Submit">Submit</button>
            <a href="" title="Cancel" tabindex="3" class="create-account">Cancel</a>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>