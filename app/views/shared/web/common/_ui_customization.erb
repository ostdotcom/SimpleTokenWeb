body{
  color: red;
}
h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6{
  color: yellow;
}
/* input box label */
.form-group .form-label{
  color: green;
}

/* input box */
.form-control{
  border: solid 1px #9bafb5;
}

/* dropdown */
.dropdown-toggle{
  border: solid 1px #9bafb5 !important;
}
a{
 color: green;
}
a:hover{
  color: purple;
  border-bottom: 1px solid purple !important;
}


input[type="checkbox"]:checked ~ label svg path{
  stroke: white;
}